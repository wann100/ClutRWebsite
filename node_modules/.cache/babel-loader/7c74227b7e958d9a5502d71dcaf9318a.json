   if (params) {\n        _.forIn(params, function (value, key) {\n          path = path.replace(value, ':' + key);\n        });\n      }\n\n      var currentRoute = this.routes.filter(function (route) {\n        return route.path === path;\n      })[0];\n      return this.getContentsByType(currentRoute.contentType, true);\n    },\n    categories: function categories() {\n      return _.countBy(this.news, 'category');\n    }\n  },\n  data: function data() {\n    return {\n      searchQuery: ''\n    };\n  },\n  methods: {\n    getListingRoute: function getListingRoute() {\n      var params = this.$route.params;\n      var path = this.$route.path;\n\n      if (params) {\n        _.forIn(params, function (value, key) {\n          path = path.replace(\"/\".concat(value), '');\n        });\n      }\n\n      return path;\n    },\n    search: function search() {\n      this.$router.replace({\n        path: this.getListingRoute(),\n        query: {\n          q: this.searchQuery\n        }\n      });\n    }\n  }\n};",{"version":3,"sources":["NewsPost.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAoFA,OAAA,WAAA;AACA,OAAA,SAAA,MAAA,4BAAA;AACA,OAAA,SAAA,MAAA,4BAAA;AACA,OAAA,CAAA,MAAA,QAAA;AAEA,eAAA;AACA,EAAA,MAAA,EAAA,CAAA,WAAA,CADA;AAEA,EAAA,UAAA,EAAA;AACA,IAAA,SAAA,EAAA,SADA;AAEA,IAAA,SAAA,EAAA;AAFA,GAFA;AAMA,EAAA,QAAA,EAAA;AACA,IAAA,WADA,yBACA;AAAA;;AACA,aAAA,KAAA,IAAA,CAAA,MAAA,CAAA,UAAA,GAAA,EAAA;AACA,eAAA,GAAA,CAAA,MAAA,CAAA,KAAA,KAAA,CAAA,MAAA,CAAA,MAAA,CAAA,EAAA,IAAA,GAAA,CAAA,IAAA,KAAA,KAAA,CAAA,MAAA,CAAA,MAAA,CAAA,EAAA;AACA,OAFA,EAEA,CAFA,CAAA;AAGA,KALA;AAMA,IAAA,IANA,kBAMA;AACA,UAAA,MAAA,GAAA,KAAA,MAAA,CAAA,MAAA;AACA,UAAA,IAAA,GAAA,KAAA,MAAA,CAAA,IAAA;;AACA,UAAA,MAAA,EAAA;AACA,QAAA,CAAA,CAAA,KAAA,CAAA,MAAA,EAAA,UAAA,KAAA,EAAA,GAAA,EAAA;AACA,UAAA,IAAA,GAAA,IAAA,CAAA,OAAA,CAAA,KAAA,EAAA,MAAA,GAAA,CAAA;AACA,SAFA;AAGA;;AACA,UAAA,YAAA,GAAA,KAAA,MAAA,CAAA,MAAA,CAAA,UAAA,KAAA,EAAA;AACA,eAAA,KAAA,CAAA,IAAA,KAAA,IAAA;AACA,OAFA,EAEA,CAFA,CAAA;AAGA,aAAA,KAAA,iBAAA,CAAA,YAAA,CAAA,WAAA,EAAA,IAAA,CAAA;AACA,KAlBA;AAmBA,IAAA,UAnBA,wBAmBA;AACA,aAAA,CAAA,CAAA,OAAA,CAAA,KAAA,IAAA,EAAA,UAAA,CAAA;AACA;AArBA,GANA;AA6BA,EAAA,IA7BA,kBA6BA;AACA,WAAA;AACA,MAAA,WAAA,EAAA;AADA,KAAA;AAGA,GAjCA;AAkCA,EAAA,OAAA,EAAA;AACA,IAAA,eADA,6BACA;AACA,UAAA,MAAA,GAAA,KAAA,MAAA,CAAA,MAAA;AACA,UAAA,IAAA,GAAA,KAAA,MAAA,CAAA,IAAA;;AAEA,UAAA,MAAA,EAAA;AACA,QAAA,CAAA,CAAA,KAAA,CAAA,MAAA,EAAA,UAAA,KAAA,EAAA,GAAA,EAAA;AACA,UAAA,IAAA,GAAA,IAAA,CAAA,OAAA,YAAA,KAAA,GAAA,EAAA,CAAA;AACA,SAFA;AAGA;;AAEA,aAAA,IAAA;AACA,KAZA;AAaA,IAAA,MAbA,oBAaA;AACA,WAAA,OAAA,CAAA,OAAA,CAAA;AAAA,QAAA,IAAA,EAAA,KAAA,eAAA,EAAA;AAAA,QAAA,KAAA,EAAA;AAAA,UAAA,CAAA,EAAA,KAAA;AAAA;AAAA,OAAA;AACA;AAfA;AAlCA,CAAA","sourcesContent":["<!-- this is an example of a dynamic template. -->\n<!-- To use this template you will need a content type which has following fields\n    img\n    title\n    date\n    subheadline\n    body\n  -->\n<template>\n  <div class=\"page-news\">\n\n    <app-header></app-header>\n\n    <div class=\"container\">\n      <div class=\"clearfix news-content\">\n        <div class=\"leftbar\">\n          <div class=\"news-preview-content\">\n            <h1 class=\"news-title\" v-text=\"currentNews.title\"></h1>\n            <p class=\"date\" v-text=\"currentNews.date\"></p>\n            <!-- Load default image if not available -->\n            <img :src=\"currentNews.img || require('@/app/assets/img/coast.jpg')\" class=\"responsive-image\">\n\n            <p class=\"subheadline\" v-text=\"currentNews.subheadline\"></p>\n\n            <div class=\"news-body\" v-html=\"currentNews.body\"></div>\n\n            <hr />\n\n            <div class=\"share-box\">\n              <h4 class=\"is-subheading\">Share this:</h4>\n\n              <ul>\n                <li>\n                  <a href=\"#\">\n                    <img src=\"@/app/assets/img/social-media/blue/facebook-blue.png\" alt=\"facebook\">\n                  </a>\n                </li>\n                <li>\n                  <a href=\"#\">\n                    <img src=\"@/app/assets/img/social-media/blue/twitter-blue.png\" alt=\"twitter\">\n                  </a>\n                </li>\n                <li>\n                  <a href=\"#\">\n                    <img src=\"@/app/assets/img/social-media/blue/pinterest-blue.png\" alt=\"pinterest\">\n                  </a>\n                </li>\n                <li>\n                  <a href=\"#\">\n                    <img src=\"@/app/assets/img/social-media/blue/instagram-blue.png\" alt=\"pinterest\">\n                  </a>\n                </li>\n              </ul>\n            </div>\n          </div>\n        </div>\n\n        <div class=\"rightbar\">\n          <div class=\"search-box form-icon-wrapper\">\n            <input @keydown.enter=\"search\" v-model=\"searchQuery\" class=\"form-control\" placeholder=\"Search...\">\n\n            <button @click=\"search\">\n              <i class=\"fa fa-arrow-right\"></i>\n            </button>\n          </div>\n\n          <h3 class=\"is-subheading\">Search By Topic</h3>\n\n          <ul v-if=\"categories\" class=\"topic-list\">\n            <li v-for=\"(count, category) in categories\" :key=\"category\">\n              <router-link :to=\"`${getListingRoute()}?cat=${category.toLowerCase()}`\">\n                {{ category }} <span class=\"count\">({{ count }})</span>\n              </router-link>\n            </li>\n          </ul>\n        </div>\n      </div>\n    </div>\n\n    <app-footer></app-footer>\n  </div>\n</template>\n\n<script>\nimport LoadContent from './LoadContent'\nimport appHeader from '@/app/components/appHeader'\nimport appFooter from '@/app/components/appFooter'\nimport _ from 'lodash'\n\nexport default {\n  mixins: [LoadContent],\n  components: {\n    appHeader,\n    appFooter\n  },\n  computed: {\n    currentNews () {\n      return this.news.filter(obj => {\n        return obj['.key'] === this.$route.params.id || obj.slug === this.$route.params.id\n      })[0]\n    },\n    news () {\n      const params = this.$route.params\n      let path = this.$route.path\n      if (params) {\n        _.forIn(params, (value, key) => {\n          path = path.replace(value, ':' + key)\n        })\n      }\n      let currentRoute = this.routes.filter((route) => {\n        return route.path === path\n      })[0]\n      return this.getContentsByType(currentRoute.contentType, true)\n    },\n    categories () {\n      return _.countBy(this.news, 'category')\n    }\n  },\n  data () {\n    return {\n      searchQuery: ''\n    }\n  },\n  methods: {\n    getListingRoute () {\n      const params = this.$route.params\n      let path = this.$route.path\n\n      if (params) {\n        _.forIn(params, (value, key) => {\n          path = path.replace(`/${value}`, '')\n        })\n      }\n\n      return path\n    },\n    search () {\n      this.$router.replace({ path: this.getListingRoute(), query: { q: this.searchQuery } })\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\">\n@import '@/app/styles/index.scss';\n\n.page-news {\n  .news-title {\n    margin-bottom: 5px;\n  }\n  .date {\n    margin-bottom: 20px;\n  }\n  .subheadline {\n    font-weight: 700;\n    margin-bottom: 25px;\n  }\n  hr {\n    margin: 30px 0 15px 0;\n  }\n  .share-box {\n    .is-subheading {\n      font-size: 20px;\n    }\n    ul {\n      display: flex;\n      margin-top: 10px;\n      li {\n        display: inline-block;\n        margin-right: 20px;\n      }\n    }\n  }\n  .search-box {\n    margin-bottom: 25px;\n  }\n}\n\n@media (max-width: 768px) {\n  .page-news {\n    .rightbar {\n      display: none;\n    }\n  }\n}\n\n@media (max-width: 600px) {\n  .page-news {\n    .search-box {\n      margin: 10px 0;\n      display: none;\n    }\n    .leftbar {\n      margin-top: 10px;\n    }\n  }\n}\n</style>\n"],"sourceRoot":"src/app/templates"}]}