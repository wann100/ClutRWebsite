     this.showModal = true;\n    },\n    confirmLogout: function confirmLogout() {\n      var _this = this;\n\n      // sign the current user out\n      firebase.auth().signOut().then(function () {\n        _this.showModal = false;\n\n        _this.$router.push('/');\n      }).catch(function () {\n        console.log('sign out fails');\n      });\n    },\n    toggleMenu: function toggleMenu() {\n      this.mobileMenuIsActive = !this.mobileMenuIsActive;\n    }\n  },\n  components: {\n    modal: modal\n  }\n};",{"version":3,"sources":["Navbar.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA6CA,OAAA,QAAA,MAAA,cAAA;AACA,OAAA,eAAA;AACA,OAAA,KAAA,MAAA,iCAAA;AACA,eAAA;AACA,EAAA,IAAA,EAAA,QADA;AAEA,EAAA,IAFA,kBAEA;AACA,WAAA;AACA,MAAA,kBAAA,EAAA,KADA;AAEA,MAAA,WAAA,EAAA,QAAA,CAAA,IAAA,GAAA,WAFA;AAGA,MAAA,SAAA,EAAA,KAHA;AAIA,MAAA,MAAA,EAAA,kCAJA;AAKA,MAAA,IAAA,EAAA,QALA;AAMA,MAAA,OAAA,EAAA,OAAA,CAAA,GAAA,CAAA,QANA;AAOA,MAAA,IAAA,EAAA,CACA;AACA,QAAA,IAAA,EAAA,SADA;AAEA,QAAA,IAAA,EAAA;AAFA,OADA,EAKA;AACA,QAAA,IAAA,EAAA,SADA;AAEA,QAAA,IAAA,EAAA;AAFA,OALA,EASA;AACA,QAAA,IAAA,EAAA,OADA;AAEA,QAAA,IAAA,EAAA;AAFA,OATA,EAaA;AACA,QAAA,IAAA,EAAA,UADA;AAEA,QAAA,IAAA,EAAA;AAFA,OAbA,EAiBA;AACA,QAAA,IAAA,EAAA,UADA;AAEA,QAAA,IAAA,EAAA;AAFA,OAjBA;AAPA,KAAA;AA8BA,GAjCA;AAkCA,EAAA,OAAA,EAAA;AACA,IAAA,OADA,qBACA;AACA,WAAA,SAAA,GAAA,IAAA;AACA,KAHA;AAIA,IAAA,aAJA,2BAIA;AAAA;;AACA;AACA,MAAA,QAAA,CAAA,IAAA,GAAA,OAAA,GACA,IADA,CACA,YAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,KAAA;;AACA,QAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA,OAJA,EAKA,KALA,CAKA,YAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,gBAAA;AACA,OAPA;AAQA,KAdA;AAeA,IAAA,UAfA,wBAeA;AACA,WAAA,kBAAA,GAAA,CAAA,KAAA,kBAAA;AACA;AAjBA,GAlCA;AAqDA,EAAA,UAAA,EAAA;AACA,IAAA,KAAA,EAAA;AADA;AArDA,CAAA","sourcesContent":["<template>\n  <nav class=\"navbar\" id=\"navbar\">\n\n    <!-- modal for post delete -->\n    <transition mode=\"out-in\" name=\"fade\">\n      <modal class=\"modal\" @close=\"showModal = false\" :kind=\"kind\" @confirmLogout='confirmLogout()' v-if=\"showModal\" :header=\"header\" />\n    </transition>\n\n    <!-- the navbar brand -->\n    <div class=\"navbar-brand\">\n      <router-link class=\"navbar-item logo\" to=\"/\">\n        <img :src=\"`${baseUrl}static/img/companylogo.png`\" alt=\"logo\">\n      </router-link>\n      <div class=\"navbar-burger burger\" data-target=\"navbar\" @click=\"toggleMenu\">\n        <span></span>\n        <span></span>\n        <span></span>\n      </div>\n    </div>\n\n    <!-- the navbar right menu -->\n    <div class=\"navbar-menu nav-burger\" id=\"navbar\" :class=\"{'is-active': mobileMenuIsActive}\">\n      <div class=\"navbar-end\">\n        <span class=\"navbar-item email\">\n          {{currentUser.email}}\n        </span>\n        <span class=\"navbar-item sign-out is-hidden-touch\" @click=\"signOut\">\n          <span class=\"icon is-medium has-text-centered\">\n            <i class=\"fa fa-bars fa-2x\"></i>\n          </span>\n        </span>\n        <!-- mobile navigation -->\n        <a class=\"navbar-item is-hidden-desktop\" @click=\"signOut\">Logout</a>\n        <div class=\"is-hidden-tablet\">\n          <router-link v-for=\"item in menu\" :key=\"item.index\" :to=\"item.path\" class=\"menu-item\">\n            <span class=\"navbar-item\">{{ item.name }}</span>\n          </router-link>\n        </div>\n      </div>\n    </div>\n\n  </nav>\n</template>\n\n<script>\nimport firebase from 'firebase/app'\nimport 'firebase/auth'\nimport modal from '@/admin/components/shared/Modal'\nexport default {\n  name: 'navbar',\n  data () {\n    return {\n      mobileMenuIsActive: false,\n      currentUser: firebase.auth().currentUser,\n      showModal: false,\n      header: 'Are you sure you want to logout?',\n      kind: 'logout',\n      baseUrl: process.env.BASE_URL,\n      menu: [\n        {\n          name: 'Content',\n          path: '/admin/content'\n        },\n        {\n          name: 'Routing',\n          path: '/admin/routing'\n        },\n        {\n          name: 'Media',\n          path: '/admin/media'\n        },\n        {\n          name: 'Database',\n          path: '/admin/database'\n        },\n        {\n          name: 'Settings',\n          path: '/admin/settings'\n        }\n      ]\n    }\n  },\n  methods: {\n    signOut () {\n      this.showModal = true\n    },\n    confirmLogout () {\n      // sign the current user out\n      firebase.auth().signOut()\n        .then(() => {\n          this.showModal = false\n          this.$router.push('/')\n        })\n        .catch(() => {\n          console.log('sign out fails')\n        })\n    },\n    toggleMenu () {\n      this.mobileMenuIsActive = !this.mobileMenuIsActive\n    }\n  },\n  components: {\n    modal\n  }\n}\n\n</script>\n\n<style lang=\"scss\">\n$navbarBg: #fff;\n$navbarColor: #aaaaaa;\n\n#navbar {\n  z-index: 1024;\n  background-color: $navbarBg;\n\n  .logo {\n    background: #fff;\n    &:hover {\n      background: whitesmoke;\n    }\n  }\n\n  .navbar-item {\n    color: $navbarColor;\n    font-weight: bold;\n    max-height: 100px;\n    &:hover {\n      color: #4bb885;\n    }\n    img {\n      max-height: 3rem;\n      margin-left: 1.2rem;\n    }\n  }\n\n  span.email {\n    margin-right: -10px;\n  }\n\n  .sign-out {\n    cursor: pointer;\n  }\n}\n\n.modal {\n  z-index: 1025;\n}\n\n.nav-burger {\n  position: fixed;\n  right: 10px;\n}\n\n@media screen and (max-width: 1023px) {\n  #navbar {\n    .navbar-menu {\n      width: auto;\n      height: auto;\n      right: 0;\n    }\n  }\n}\n</style>\n"],"sourceRoot":"src/admin/components/layout"}]}